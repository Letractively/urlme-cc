<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <link rel="icon" href="http://i.imgur.com/PLuBQfs.gif" />

    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("//cdn.ianhd.com/css/common.css")
    
    @* jQuery and jQueryUI *@
    @Scripts.Render("~/bundles/jquery")
    @Styles.Render("//ajax.googleapis.com/ajax/libs/jqueryui/1/themes/smoothness/jquery-ui.min.css")
    @Scripts.Render("//ajax.googleapis.com/ajax/libs/jqueryui/1/jquery-ui.min.js")

    @Scripts.Render("//cdn.ianhd.com/js/ianhd.js")

    @* knockout *@
    @Scripts.Render("//cdn.ianhd.com/plugins/knockout/knockout-2.2.1.js")
    @Scripts.Render("//cdn.ianhd.com/plugins/knockout/knockout.mapping.js")

    @* plugins *@
    @Scripts.Render("//cdn.ianhd.com/plugins/shortcut.js")
    @Scripts.Render("//cdn.ianhd.com/plugins/date.js")
</head>
<body>
    <div class="body">
        <h1><a href="~/">admin.ianhd.com</a></h1>
        @RenderBody()
    </div>    

    @RenderSection("scripts", required: false)
</body>
</html>
