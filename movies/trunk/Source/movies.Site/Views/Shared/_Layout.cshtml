<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/content/site.css")" rel="stylesheet"/>
    <link href="@Url.Content("~/content/jquery.mobile-1.0.1.min.css")" rel="stylesheet" />
    <script src="@Url.Content("~/scripts/jquery-1.7.1.min.js")"></script>
    <script src="@Url.Content("~/scripts/codejkjk/codejkjk.js")"></script>
    <script src="@Url.Content("~/scripts/plugins/jsrender.js")"></script>
    <script type="text/javascript">

        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-26494629-1']);
        _gaq.push(['_trackPageview']);

        (function () {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();

    </script>
    @RenderSection("ExtraHeadContent", required: false)
</head>

<body>
    @RenderBody()

    <script id="navTemplate" type="text/x-jquery-tmpl">
        <a href="#" class="{{=className}}">{{=text}}</a>
    </script>
</body>
</html>
